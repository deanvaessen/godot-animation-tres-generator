{
  "name": "typescript-microservice",
  "version": "0.2.0",
  "scripts": {
    "pretest_temp_disabled": "./node_modules/.bin/eslint ./src/",
    "start": "npm run dev",
    "dev": "NODE_ENV=dev npm run init-dev",
    "staging": "NODE_ENV=staging npm run init-prod",
    "prod": "NODE_ENV=prod npm run init-prod",
    "init-prod": "pm2 -f start init_prod.json --no-daemon",
    "init-dev": "pm2 -f start init_dev.json --no-daemon --watch",
    "test": "jest --watchAll",
    "test:verbose": "jest --watchAll --verbose false"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.4",
    "@types/express": "^4.17.0",
    "@types/fs-extra": "^8.0.0",
    "@types/jest": "^24.0.9",
    "@types/klaw": "^3.0.0",
    "@types/node": "^11.13.14",
    "@types/through2": "^2.0.34",
    "babel-preset-env": "^1.7.0",
    "chokidar": "^3.2.1",
    "fs-extra": "^8.1.0",
    "inversify": "^5.0.1",
    "jest": "^24.5.0",
    "klaw": "^3.0.0",
    "pm2": "^2.10.4",
    "reflect-metadata": "^0.1.13",
    "through2": "^3.0.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.3.4000"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "babel-eslint": "^8.2.6",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-prettier": "^3.0.1",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.10.0",
    "ts-jest": "^24.0.2"
  }
}
